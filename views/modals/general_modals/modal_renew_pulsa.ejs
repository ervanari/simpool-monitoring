<div class="modal fade" id="req_expdate" tabindex="-1" aria-labelledby="req_expdate" aria-hidden="true">
	<div class="modal-dialog">
		<div class="modal-content">
			<form action="/renewpulsa" method="post">
				<div class="modal-header">
					<h5 class="modal-title" id="newKeyModals">Cek Masa AKtif</h5>
					<button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
				</div>
				<div class="modal-body justify-content-center p-4 d-none">
					<input type="text" name="mode" placeholder="status" id="mode" class="w-100 p-2 rounded border"
						required>
				</div>
				<div class="modal-body justify-content-center p-4 d-none">
					<input type="text" name="port" placeholder="status" id="port" class="w-100 p-2 rounded border"
						required>
				</div>
				<div class="modal-body justify-content-center p-4">
					<div>Please type code ussd to confirm.</div>
					<input type="text" name="code" placeholder="Example *888*8#" id="code"
						class="w-100 p-2 rounded border" required>
					<div class="text-end bottom-0">Estimasi update 10 detik, Harap Menunggu ...!</div>
				</div>
				<div class="modal-footer">
					<button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Cancel</button>
					<button type="submit" class="btn btn-primary">Save</button>
				</div>
			</form>
		</div>
	</div>
</div>


