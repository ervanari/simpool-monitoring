<div class="modal fade" id="restart_services" tabindex="-1" aria-labelledby="restart_services" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <form action="/service/control_service" method="post">
                <div class="modal-header">
                    <h5 class="modal-title" id="newKeyModals">Restart</h5>
                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                </div>
                <div class="modal-body justify-content-center p-4">
                    <div>Choice Action.</div>
                    <div class="form-group">
                        <select class="form-control" id="run" name="run" onchange="onchangeOption(this)">
                            <option value="0">-- Select --</option>
                            <option id="startservice" value="start">Start</option>
                            <option value="stop">Stop</option>
                            <option value="restart">Restart</option>
                        </select>
                    </div>
                    <div class="modal-body justify-content-center p-4 d-none">
                        <input type="text" name="keyser" placeholder="" id="key_service"
                            class="w-100 p-2 rounded border" required>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Cancel</button>
                    <button type="submit" class="btn btn-primary">Submit</button>
                </div>
            </form>
        </div>
    </div>
</div>